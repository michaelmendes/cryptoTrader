package cryptoTrader.utils;


public class TradingBroker {
	private String name;
	private CryptocoinList cryptoList;
	private String strategy;
	
	public TradingBroker() {
		name = null;
		cryptoList = null;
		strategy = null;
	}
	
	public void setName(String brokerName) {
		name = brokerName;
	}
	
	public void setList(CryptocoinList list) {
		cryptoList = list;
	}
	
	public void setStrategy(String newStrategy) {
		strategy = newStrategy;
	}
	
	public void declareInterest(Cryptocoin coinOfInterest) {
		cryptoList.addCoin(coinOfInterest);
	}
	
	public void attachStrategy() {
		
	}
	
}
